{
  "name": "yellfy-svg-sprite",
  "version": "1.0.0-alpha.0",
  "description": "A very simple module to generate SVG sprites.",
  "license": "MIT",
  "repository": "mrmlnc/yellfy-svg-sprite",
  "author": {
    "name": "Denis Malinochkin",
    "url": "canonium.com"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "pretest": "rimraf out .tmp && tslint src/**/*.ts && tsc",
